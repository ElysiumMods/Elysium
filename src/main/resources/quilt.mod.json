{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.enderio",
    "id": "enderio",
    "version": "${version}",
    "metadata": {
      "name": "Elysium",
      "description": "An unofficial quilt port of EnderIO.",
      "contributors": {
        "Ryzech": "",
        "Ecorous": ""
      },
      "contact": {
        "issues": "https://github.com/ElysiumMods/Elysium/issues",
        "sources": "https://github.com/ElysiumMods/Elysium/"
      },
      "icon": "assets/enderio/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "com.enderio.EnderIO"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.6-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=4.0.0"
      },
      {
        "id": "minecraft",
        "versions": "1.19.2"
      }
    ]
  },
  "mixin": "enderio.mixins.json"
}
